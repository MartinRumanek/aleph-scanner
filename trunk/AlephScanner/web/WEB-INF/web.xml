<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Handle</servlet-name>
        <servlet-class>cz.mzk.alephscanner.Handle</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Handle2</servlet-name>
        <servlet-class>cz.mzk.alephscanner.Handle2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetInfo</servlet-name>
        <servlet-class>cz.mzk.alephscanner.GetInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Handle</servlet-name>
        <url-pattern>/Handle</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Handle2</servlet-name>
        <url-pattern>/Handle2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetInfo</servlet-name>
        <url-pattern>/GetInfo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
